App.Router.map(function() {
  
});


// Route par dÃ©faut
App.ApplicationRoute = Em.Route.extend({
    model:  function() {
    	var configs = this.store.find('Config');
    	if (configs.length >= 1) {
    		return configs.objetAtContent(0);
    	} else {
    		var config = store.createRecord('Config', {currentProject: null});
			return config;
    	}
   } 
});




